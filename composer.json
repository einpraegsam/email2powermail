{
  "name": "in2code/email2powermail",
  "description": "Convert Email-Addresses in TYPO3 Frontend to links to a powermail form. The form will be send to this email.",
  "keywords": ["typo3", "form", "mailform", "spamprevention"],
  "homepage": "https://github.com/einpraegsam/email2powermail",
  "authors": [
    {
      "name": "Alex Kellner",
      "email": "alexander.kellner@in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-3.0",
  "require": {
    "typo3/cms-core": ">=7.6.0 <9.0.0",
    "in2code/powermail": ">=4.0.0 <5.0.0"
  },
  "replace": {
    "email2powermail": "self.version",
    "typo3-ter/email2powermail": "self.version"
  },
  "autoload": {
    "psr-4": {
      "In2code\\Email2powermail\\": "Classes/"
    }
  }
}
